                                        </li>
                                        <li>
                                            <a href="#">Settings 2</a>
                                        </li>
                                    </ul>
                                </li>*@
                            <li>
                                <a class="close-link"><i class="fa fa-close"></i></a>
                            </li>

                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <div style="overflow-x:auto">
                            <table id="datatable-responsive" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Person_Key)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Staff_Name)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Last_Name)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.First_Name)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Middle_Initial)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Alias)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Location)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Cell_Phone)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Personal_Cell_Phone)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Desk_Phone)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Company_Email)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.IsForeign_National)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.IsBillable)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Years_of_Experience)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Hire_Date)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Exit_Date)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Exit_Note)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Is_Eligible_for_Rehire)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Created_On)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Created_By)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Modified_On)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Modified_By)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Adm_Exit_Reason.Exit_Reason)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Adm_Prefix.Prefix)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Adm_Suffix.Suffix)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Ref_Company.Company)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Staff_Clearance.Created_By)
                                        </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Person_Key)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Staff_Name)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Last_Name)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.First_Name)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Middle_Initial)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Alias)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Location)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Cell_Phone)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Personal_Cell_Phone)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Desk_Phone)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Company_Email)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.IsForeign_National)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.IsBillable)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Years_of_Experience)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Hire_Date)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Exit_Date)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Exit_Note)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Is_Eligible_for_Rehire)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Created_On)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Created_By)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Modified_On)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Modified_By)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Adm_Exit_Reason.Exit_Reason)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Adm_Prefix.Prefix)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Adm_Suffix.Suffix)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Ref_Company.Company)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Staff_Clearance.Created_By)
                                            </td>
                                            <td>
                                                @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                                                @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                                                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Datatables -->
    <script src="../vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="../vendors/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <script src="../vendors/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="../vendors/datatables.net-buttons-bs/js/buttons.bootstrap.min.js"></script>
    <script src="../vendors/datatables.net-buttons/js/buttons.flash.min.js"></script>
    <script src="../vendors/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="../vendors/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="../vendors/datatables.net-fixedheader/js/dataTables.fixedHeader.min.js"></script>
    <script src="../vendors/datatables.net-keytable/js/dataTables.keyTable.min.js"></script>
    <script src="../vendors/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="../vendors/datatables.net-responsive-bs/js/responsive.bootstrap.js"></script>
    <script src="../vendors/datatables.net-scroller/js/dataTables.scroller.min.js"></script>
    <script src="../vendors/jszip/dist/jszip.min.js"></script>
    <script src="../vendors/pdfmake/build/pdfmake.min.js"></script>
    <script src="../vendors/pdfmake/build/vfs_fonts.js"></script>
</div>
